FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

4
(
    upface
    {
        type            patch;
        nFaces          1;
        startFace       49999;
    }
    downface
    {
        type            patch;
        nFaces          1;
        startFace       50000;
    }
    fixedWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          100000;
        startFace       50001;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          100000;
        startFace       150001;
    }
)

